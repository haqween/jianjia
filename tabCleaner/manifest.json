{
  "name": "TabCleaner",
  "description": "No Manager! Auto Close!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["history","storage", "activeTab", "scripting", "tabs", "bookmarks"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "commands": {
    "toPreTab": {
      "suggested_key": {
        "default": "Alt+Z",
        "mac": "MacCtrl+Shift+X"
      },
      "description": "跳转到上一个标签页",
      "global": true
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "options_page": "options.html"
}
